<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="PrjVinculo.MainPage">

    <ScrollView>
        <VerticalStackLayout
            Padding="120,0"
            Spacing="25">

            <Label
                Margin="25"
                Text="Celulares"
                Style="{StaticResource Headline}"
                SemanticProperties.HeadingLevel="Level1" />

            <Button
                Text="Celular 1"
                x:Name="btnCel01"
                Clicked="btnCel01_Clicked" />
            <Button
                Text="Celular 2"
                x:Name="btnCel02"
                Clicked="btnCel02_Clicked" />
            <Button
                Text="Celular 3"
                x:Name="btnCel03"
                Clicked="btnCel03_Clicked" />
            <Button
                Text="Celular 4"
                x:Name="btnCel04"
                Clicked="btnCel04_Clicked" />
        </VerticalStackLayout>
    </ScrollView>

</ContentPage>
